Order:
1) cat start.txt
2) cp source/* HW/00
3) cd 00
4) cat hint1.txt
5) make encode ARGS="hint.txt key1.txt output"
6) hexdump output
7) cp source/* HW/4a
8) cd 4a
9) cat hint2.txt
10) make decode ARGS="ciphertext2 key2.txt output"
11) cp source/* HW/84
12) cd 84
13) cat hint3.txt
14) find . -name "f1@g"
15) cd fb
16) echo "PRGA" > PRGA
17) make decode ARGS="f1@g PRGA output"
18) cat output

